<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="KoelApi" >
    <listeners>
        <listener class-name="util.listeners.TestListener"/>
    </listeners>
    <test name="Koel Api Tests" preserve-order="false">
        <parameter name="baseURL" value="https://qa.koel.app"/>
        <classes>
            <class name="apitests.PlaylistTests"/>
             <class name="apitests.UserTests"/>
            <class name="apitests.SongsTests"/>
            <class name="apitests.LoginTests"/>
        </classes>
    </test>
</suite>