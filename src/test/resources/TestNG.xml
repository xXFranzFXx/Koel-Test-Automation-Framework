<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="TestNG" parallel="classes" thread-count="4" verbose="1">
    <listeners>
        <listener class-name="util.listeners.TestListener"/>
        <listener class-name="util.listeners.AnnotationTransformer"/>
    </listeners>
    <test name="TestNG" preserve-order="false">
        <parameter name="baseURL" value="https://qa.koel.app/"/>
        <classes>
            <class name="testcases.LoginTests"/>
            <class name="testcases.HomeTests"/>
            <class name="testcases.AlbumsTests"/>
            <class name="testcases.AlbumsTests"/>
            <class name="testcases.ArtistsTests"/>
            <class name="testcases.CurrentQueueTests"/>
            <class name="testcases.FavoritesTests"/>
            <class name="testcases.InfoPanelTests"/>
            <class name="testcases.LogoutTests"/>
            <class name="testcases.ProfileTests"/>
            <class name="testcases.SearchTests"/>
            <class name="testcases.SmartPlaylistsTests"/>
        </classes>
    </test>
</suite>